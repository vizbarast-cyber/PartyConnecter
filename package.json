{
  "name": "partyconnect",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "backend": "cd backend && npm start",
    "dev": "concurrently \"npm start\" \"npm run backend\"",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "setup:production": "node scripts/setup-production.js",
    "generate:jwt": "node scripts/generate-jwt-secret.js",
    "deploy:firebase-rules": "node scripts/deploy-firebase-rules.js",
    "build:first": "powershell -ExecutionPolicy Bypass -File scripts/first-build.ps1",
    "eas:login": "npx eas-cli login",
    "eas:configure": "npx eas-cli build:configure"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@stripe/stripe-react-native": "~0.35.1",
    "axios": "^1.6.2",
    "date-fns": "^3.0.0",
    "expo": "^54.0.23",
    "expo-asset": "~12.0.9",
    "expo-auth-session": "~7.0.8",
    "expo-camera": "~17.0.9",
    "expo-constants": "~18.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-location": "~19.0.7",
    "expo-notifications": "~0.32.12",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.9",
    "firebase": "^10.14.1",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-deck-swiper": "^2.0.13",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-worklets": "0.5.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "concurrently": "^8.2.2"
  },
  "expo": {
    "install": {
      "exclude": [
        "@stripe/stripe-react-native"
      ]
    }
  },
  "private": true
}
